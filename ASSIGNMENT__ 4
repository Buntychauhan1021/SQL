SELECT * from EMPLOYEE;
SELECT ASCII('EMPNAME') FROM EMPLOYEE;
SELECT CONCAT('RAJKUMAR002', 'Javatpoint') FROM EMPLOYEE;  
SELECT DIFFERENCE('001RAJKUMAR'), DIFFERENCE('RAVIKUMAR003') FROM EMPLOYEE;
SELECT LEFT('RAVIKUMAR003', 5) FROM EMPLOYEE;  

CREATE TABLE EMPLOYEE1(FIRSTNAME VARCHAR(20),LASTNAME VARCHAR(20));
INSERT INTO EMPLOYEE1 VALUES('PANKAJ','TIWARI');
INSERT INTO EMPLOYEE1 VALUES('ASHOK','SHARMA');
INSERT INTO EMPLOYEE1 VALUES('ARUN','GUPTA');

SELECT * FROM EMPLOYEE1;

SELECT FIRSTNAME ||' '||LASTNAME FROM EMPLOYEE1;

CREATE TABLE EMPLOYEE2(FIRSTNAME VARCHAR(20),MIDDLENAME VARCHAR(20),LASTNAME VARCHAR(20));

INSERT INTO EMPLOYEE2 VALUES('PANKAJ','KUMAR','TIWARI');
INSERT INTO EMPLOYEE2 VALUES('ASHOK','KUMAR','SHARMA');
INSERT INTO EMPLOYEE2 VALUES('ARUN','KUMAR','GUPTA');
SELECT FIRSTNAME ||''||MIDDLENAME||''||LASTNAME FROM EMPLOYEE2;

SELECT * FROM EMPLOYEE2;

SELECT SUBSTR(FIRSTNAME,1,1) ||'.'||SUBSTR(MIDDLENAME,1,1)||''||'.'||SUBSTR(LASTNAME,1) FROM EMPLOYEE2;

CREATE TABLE EMPLOYEE4(SNAME VARCHAR(20),MARKS VARCHAR(20));

INSERT INTO EMPLOYEE4 VALUES('RAJ','65');
INSERT INTO EMPLOYEE4 VALUES('AMIT','32');
INSERT INTO EMPLOYEE4 VALUES('SANJAY','45');
INSERT INTO EMPLOYEE4 VALUES('ROHIT','40');
INSERT INTO EMPLOYEE4 VALUES('ANIL','35');

SELECT * FROM EMPLOYEE4;
SELECT SNAME, CASE WHEN MARKS >=35 THEN 'PASS' 
ELSE 'FAIL'
END AS RESULT 
FROM EMPLOYEE4;
